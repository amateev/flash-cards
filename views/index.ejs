<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Flash Cards</title>

  <!-- bootstrap goes first-->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  
  <!-- reset css -->
  <link rel="stylesheet" type="text/css" href="assets/css/reset.css">

  <!-- style css -->
  <!-- /style.css is the samething as
  http://localhost:3000/style.css
  but /style.css is better because it works for any port number and any domain name -->
  <link rel="stylesheet" type="text/css" href="assets/css/style.css">
</head>

<body>
  <!-- navigation bar -->
  <nav class="navbar navbar-inverse">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1"
                    aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/">Flash Cards</a>
            </div>

        </div>
        <!-- /.container-fluid -->
    </nav>
    <!-- end of nav bar -->

<!--Wrap main contant in a div .wrap-->
<div id="wrap">
  
    <div class="container">
        <div class="row">
            <div class="col-sm-3">
                <div class="list-group">
                      <a href="http://localhost:3000/" class="list-group-item list-group-item-action active">My Sets of Cards</a>
                      <a href="http://localhost:3000/new-card" class="list-group-item list-group-item-action">Create a New Flash Card</a>
                      <a href="http://localhost:3000/cards" class="list-group-item list-group-item-action">View all Cards</a>
                      <a href="http://localhost:3000/cards-edit" class="list-group-item list-group-item-action">Edit Cards</a>
                </div>
        </div>

        <div class="col-sm-9">
               <div class="jumbotron jumbotron-fluid">
                                
                  <h1 class="display-4">My Sets of Cards</h1>
                  <br>
                  <hr class="my-4">

                  <p class="lead">Select your Set of Cards</p>
                  
                  <div class="subjects">
                    <% for (var i=0; i<question.length; i++) { %>
                      <button id='subject-btn'><%=   question[i].subject  %></button><% } %>




                  </div>

                  <hr class="my-4">

                  <a href="/new-card">Create a new Set of Cards</a>
           
                  
                
        </div></div>
    </div>
    </div>
</div>










  
  



<!--Start of the Footer-->
<footer class="footer">
    <div class="container">
          <span class="text-muted">Copyright &copy</span> 
    </div>
</footer>
   
    <!--End of the Footer-->


  <!-- jquery link -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
